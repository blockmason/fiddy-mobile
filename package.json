  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "lib/"
    ],
    "cacheDirectory": ".jest/cache",
    "moduleNameMapper": {
      "^actions": "<rootDir>/packages/actions",
      "^credit-protocol": "<rootDir>/packages/credit-protocol",
      "^language": "<rootDir>/packages/language",
      "^navigators": "<rootDir>/packages/navigators",
      "^lndr/(.*)": "<rootDir>/packages/lndr/$1",
      "^reducers": "<rootDir>/packages/reducers",
      "^reducers/(.*)": "<rootDir>/packages/reducers/$1",
      "^store": "<rootDir>/packages/store",
      "^theme/(.*)": "<rootDir>/packages/theme/$1",
      "^ui/(.*)": "<rootDir>/packages/ui/$1",
      "^images/(.*)": "<rootDir>/packages/images/$1"
    }
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  }
